@model IEnumerable<LivrariaTJ.Models.Livro>

@{
    ViewBag.Title = "Galeria";
}

<h2>Galeria</h2>

<div class="row">

    @foreach (var item in Model)
    {

        <div class="col-md-4">
            <img src="@Url.Content(item.ImagemCapa)" alt="IMAGES" width="300" height="400" />
        </div>
    }
</div>
